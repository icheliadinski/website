{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.11",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.2",
    "@types/react-transition-group": "4.2.3",
    "node-sass": "4.13.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react-transition-group": "4.3.0",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:prod": "yarn test --watchAll=false --coverage",
    "lint": "stylelint 'src/**/*.scss'",
    "check": "yarn run lint && yarn run test:prod",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "storybook": "start-storybook",
    "build:storybook": "build-storybook -c .storybook -o build/storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/@app/Root.tsx",
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}"
    ]
  },
  "devDependencies": {
    "@storybook/react": "5.2.6",
    "@storybook/theming": "5.2.6",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "coveralls": "3.0.8",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "husky": "3.1.0",
    "prettier": "1.19.1",
    "prettier-tslint": "0.4.2",
    "stylelint": "12.0.0",
    "stylelint-order": "3.1.1",
    "tslint": "5.20.1",
    "tslint-react": "4.1.0",
    "tslint-react-hooks": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run check"
    }
  }
}
